<h1>Users [Admin Page]</h1>
<table id="users">
  <thead>
    <tr>
      <th>User Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.full_name, admin_user_path(user) %></td>
        <td><%= user.email %></td>
      </tr>
  <% end %>
  </tbody>
</table>
